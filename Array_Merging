#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n1,n2;
    cout<<"Enter the size of  array:";
    cin>>n1;
    cout<<"Enter the size of  array:";
    cin>>n2;
    int a[n1],b[n2];
    for(int i=0;i<n1;i++){
        cin>>a[i];
    }
    for(int i=0;i<n2;i++){
        cin >>b[i];
    }

    for(int i=0;i<n2;i++){
        a[n1+i]= b[i];
    }
    for(int i=0;i<n1+n2;i++){
        cout<<a[i]<<" ";
    }
}
